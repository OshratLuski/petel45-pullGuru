{{!
    @template mod_quiz/summary_table

    Summary attempt data

    Classes required for JS:
    * none

    Data attributes required for JS:
    * none

    Context variables required for this template:
    * none

    Example context (json):
    {
        "rows": [{
            "title": "Status",
            "content": "Finished",
        }]
    }
}}

<div class="generaltable generalbox quizreviewsummary mb-5">
    <div class="container-fluid">
        {{#rows}}
        <div class="rowgroup">
            <div class="row border-top">
                <div aria-hidden = "true" class="col-5 col-lg-2 bold" style="background-color: #f0f0f0;">{{{ title }}}</div>
                <div aria-description = "{{title}}" class='col-7 col-lg-10 ' style="background-color: #fafafa;">{{{ content }}}</div>
            </div>
        </div>
        {{/rows}}
    </div>
</div>