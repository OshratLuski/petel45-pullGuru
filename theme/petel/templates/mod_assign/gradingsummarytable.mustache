{{!
    @template mod_assign/gradingsummarytable

    Summary attempt data

    Classes required for JS:
    * none

    Data attributes required for JS:
    * none

    Context variables required for this template:
    * none

    Example context (json):
    {
        "notification": "Assign notification",
        "rows": [{
            "cell1content": "Hidden from students",
            "cell2content": "No",
        }],
        "submissionlink": [{"url" : "moodle_url"}]
        "cangradelink": [{"url" : "moodle_url"}]
    }
}}

<div class="groupsprint mb-2">
    {{{groupsprint}}}
</div>

<div class="gradingsummary mb-5">
    <div class="container-fluid" aria-label="{{#str}}gradingsummary, assign{{/str}}" aria-describedby="gradingsummarytabletitle">
        <div class="row mb-4">
            <h3 id="gradingsummarytabletitle" class="gradingsummarytabletitle col mb-0 px-0">{{#str}}gradingsummary, assign{{/str}}</h3>
        </div>
        {{#notification}}<div class="row"> {{{notification}}} </div>{{/notification}}
        {{#rows}}
            <div role="row" class="row py-2 border-top">
                <div role="rowheader" class="col-5 bold py-1">{{cell1content}}</div>
                <div role="cell" class="col-7 py-1">{{cell2content}}</div>
            </div>
        {{/rows}}
    </div>
</div>
