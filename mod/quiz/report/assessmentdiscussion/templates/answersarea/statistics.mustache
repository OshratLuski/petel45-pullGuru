<div class="students_answers_distribution mb-3">
    <p class="students_answers_distribution-title  text-body bold mb-3 text-center">
        {{#str}}distributionofstudentsanswers, quiz_assessmentdiscussion {{/str}}
    </p>
    <div class="d-flex">
        <div class="subtitles">
            <div class="subtitle-line d-flex align-items-center text-body mb-4">
                <div class="subtitle-icon correct mr-3"></div>
                <span class="text-body">מסיח ראשון</span>
            </div>
            <div class="subtitle-line d-flex align-items-center text-body mb-4">
                <div class="subtitle-icon uncorrect mr-3"></div>
                <span class="text-body">מסיח שני</span>
            </div>
            <div class="subtitle-line d-flex align-items-center text-body">
                <div class="subtitle-icon uncorrect mr-3"></div>
                <span class="text-body">מסיח שלישי</span>
            </div>
        </div>
        <div class="progress-wrapper">
            <div class="progress-line d-flex align-items-center mb-4">
                <div class="progress-bar mr-2 bg-white">
                    <div class="progress-inner" style="width: 65%;"></div>
                </div>
                <span class="text-body">65%</span>
            </div>
            <div class="progress-line d-flex align-items-center mb-4">
                <div class="progress-bar mr-2 bg-white">
                    <div class="progress-inner" style="width: 5%;"></div>
                </div>
                <span class="text-body">5%</span>
            </div>
            <div class="progress-line d-flex align-items-center">
                <div class="progress-bar mr-2 bg-white">
                    <div class="progress-inner" style="width: 25%;"></div>
                </div>
                <span class="text-body">25%</span>
            </div>
        </div>
    </div>
</div>