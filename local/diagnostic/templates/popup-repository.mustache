<div class="modal" id="local_diagnostic_repository_modal" data-backdrop="static">
    <div class="modal-dialog popup-local-diagnostic">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title">{{#str}}repositorypopuptitle, local_diagnostic{{/str}}</h5>
                <button type="button" aria-label="Close" class="close popup-close" data-dismiss="modal">
                    <span aria-hidden="true">Ã—</span>
                </button>
            </div>
            <div class="modal-body-repository">
                <div class="activities-diagnostic-title">{{#str}}repositorytoptitle, local_diagnostic{{/str}} <b>{{clusterName}}</b></div>
                <iframe id="repository_iframe" src="" frameborder="0"></iframe>
            </div>
            <div class="repository-footer">
                <button type="button" data-dismiss="modal" class="btn btn-primary m-1">{{#str}}cancel,
                    local_diagnostic{{/str}}</button>
                <button id="repository_submit" type="button" class="btn btn-success m-1">{{#str}}submit,
                    local_diagnostic{{/str}}</button>
            </div>
        </div>
    </div>
</div>