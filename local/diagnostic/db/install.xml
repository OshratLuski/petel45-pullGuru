<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="local/diagnostic/db" VERSION="20161026" COMMENT="XMLDB file for Moodle local/diagnostic"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../../../lib/xmldb/xmldb.xsd"
>
  <TABLES>
    <TABLE NAME="local_diagnostic_cache" COMMENT="Cache data table">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true"/>
        <FIELD NAME="mid" TYPE="int" LENGTH="10" NOTNULL="true" DEFAULT="0" SEQUENCE="false" COMMENT="mid"/>
        <FIELD NAME="rebuild" TYPE="int" LENGTH="1" NOTNULL="true" DEFAULT="0" SEQUENCE="false" COMMENT="recache with R"/>
        <FIELD NAME="readytouse" TYPE="int" LENGTH="1" NOTNULL="true" DEFAULT="0" SEQUENCE="false" COMMENT="is it ready to display"/>
        <FIELD NAME="data" TYPE="binary" LENGTH="big" NOTNULL="false" SEQUENCE="false" COMMENT="Blob data"/>
        <FIELD NAME="extra" TYPE="binary" LENGTH="big" NOTNULL="false" SEQUENCE="false" COMMENT="Blob data"/>
        <FIELD NAME="centroids" TYPE="binary" LENGTH="small" NOTNULL="false" SEQUENCE="false" COMMENT="Blob centroid data"/>
        <FIELD NAME="extracentroids" TYPE="binary" LENGTH="small" NOTNULL="false" SEQUENCE="false" COMMENT="Extra blob centroid data"/>
        <FIELD NAME="activities" TYPE="binary" LENGTH="medium" NOTNULL="false" SEQUENCE="false" COMMENT="Selectbox data with courses activities and attempts"/>
        <FIELD NAME="buildtime" TYPE="char" LENGTH="10" NOTNULL="false" SEQUENCE="false" COMMENT="Build time"/>
        <FIELD NAME="bradclusternum" TYPE="int" LENGTH="2" NOTNULL="false" SEQUENCE="false" COMMENT="Brad cluster num"/>
        <FIELD NAME="attempts" TYPE="int" LENGTH="10" NOTNULL="false" SEQUENCE="false" COMMENT="Successful attempts"/>
        <FIELD NAME="usermodified" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="false"/>
        <FIELD NAME="timecreated" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="false"/>
        <FIELD NAME="timemodified" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="false"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
        <KEY NAME="mid" TYPE="unique" FIELDS="mid"/>
      </KEYS>
    </TABLE>
    <TABLE NAME="local_diagnostic_brad" COMMENT="Brad predictions table">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true"/>
        <FIELD NAME="mid" TYPE="int" LENGTH="10" NOTNULL="true" DEFAULT="0" SEQUENCE="false" COMMENT="mid"/>
        <FIELD NAME="bradclusternum" TYPE="int" LENGTH="2" NOTNULL="false" SEQUENCE="false" COMMENT="Brad cluster num"/>
        <FIELD NAME="allbradclusters" TYPE="char" LENGTH="100" NOTNULL="false" SEQUENCE="false" COMMENT="All brad clusters"/>
        <FIELD NAME="bradmin" TYPE="int" LENGTH="2" NOTNULL="false" SEQUENCE="false" COMMENT="Brad cluster num"/>
        <FIELD NAME="bradmax" TYPE="int" LENGTH="2" NOTNULL="false" SEQUENCE="false" COMMENT="Brad cluster num"/>
        <FIELD NAME="attempts" TYPE="int" LENGTH="10" NOTNULL="false" SEQUENCE="false" COMMENT="Successful attempts"/>
        <FIELD NAME="questions" TYPE="int" LENGTH="10" NOTNULL="false" SEQUENCE="false" COMMENT="Number of questions used"/>
        <FIELD NAME="usermodified" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="false"/>
        <FIELD NAME="timecreated" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="false"/>
        <FIELD NAME="timemodified" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="false"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
        <KEY NAME="bradclusternummid" TYPE="unique" FIELDS="mid,bradclusternum"/>
      </KEYS>
    </TABLE>
  </TABLES>
</XMLDB>