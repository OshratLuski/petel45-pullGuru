<div class="modal-header">
    <h5 class="modal-title">{{#str}}give_feedback, community_oer {{/str}} {{activityName}}</h5>
    <button id="confirmClose" type="button" class="close" data-dismiss="modal" aria-label="Close">
        <span aria-hidden="true">Ã—</span>
    </button>
</div>

<div class="modal-body">
    <p class="modal-body-title">{{#str}}text_confirm_reject, community_oer {{/str}}</p>
</div>

<div class="modal-footer" style="min-height: 69px;">
    <button data-handler="rejectReview" type="button" class="btn btn-outline-primary btn-sm align-self-baseline"
            data-requestid="{{requestId}}" data-dismiss="modal">
        {{#str}}dont_wont_comment_activity, community_oer {{/str}}
    </button>
    <button data-dismiss="modal" data-activityid="{{activityId}}" data-requestid="{{requestId}}"
            data-handler="sendReviewLater" type="button" class="btn btn-outline-primary btn-sm align-self-baseline">
        {{#str}}remind_me_later, community_oer {{/str}}
    </button>
</div>
