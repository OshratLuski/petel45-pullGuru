<div id="copy_from_my_courses_{{uniqueid}}" class="copy-from-my-courses">

    <div class="modal-inner pt-2">
        <div class="form-row mb-1 mx-0">
            <div class="form-group d-flex align-items-center justify-content-between w-100 position-relative mb-0">
                <label class="form-control-label question-search-label mb-0" for="search_{{uniqueid}}">
                    {{#str}}describe_copy_question, community_sharequestion{{/str}}
                </label>
                <div class="petel-search">
                    <input id="search_{{uniqueid}}" type="text" class="form-control question-search-input petel-search-input"
                        placeholder="{{#str}}qsearch, community_sharequestion{{/str}}">
                    <div class="position-absolute border-0 petel-search-icon">
                        <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" viewBox="0 0 16 16" fill="none">
                            <path d="M15.7824 13.8234L12.6666 10.7081C12.5259 10.5674 12.3353 10.4893 12.1353 10.4893H11.6259C12.4884 9.38631 13.001 7.99895 13.001 6.48972C13.001 2.89945 10.0914 -0.00964355 6.50048 -0.00964355C2.90959 -0.00964355 0 2.89945 0 6.48972C0 10.08 2.90959 12.9891 6.50048 12.9891C8.00996 12.9891 9.39756 12.4766 10.5008 11.6142V12.1235C10.5008 12.3235 10.5789 12.5141 10.7195 12.6547L13.8354 15.7701C14.1292 16.0638 14.6042 16.0638 14.8948 15.7701L15.7793 14.8858C16.0731 14.5921 16.0731 14.1171 15.7824 13.8234ZM6.50048 10.4893C4.29094 10.4893 2.50018 8.70201 2.50018 6.48972C2.50018 4.28056 4.28781 2.49011 6.50048 2.49011C8.71001 2.49011 10.5008 4.27744 10.5008 6.48972C10.5008 8.69888 8.71314 10.4893 6.50048 10.4893Z" fill="#554283"/>
                        </svg>
                    </div>
                </div>
            </div>

        </div>

        <p class="checked-question-counter bold mb-0">
            {{#str}}questionchecked, community_sharequestion{{/str}} <span
                id="checked-question-counter-num-{{uniqueid}}">0</span>
        </p>

        <div class="inner-heading d-flex align-items-center bg-primary text-white p-2 mb-4">

            <div class="custom-control custom-checkbox question-custom-checkbox checkbox-cell px-2 d-flex
                        align-items-center justify-content-center border-right border-secondary">

                <input type="checkbox" class="custom-control-input select-all-checkboxes"
                    id="all_checkboxes_{{uniqueid}}">
                <label class="custom-control-label " for="all_checkboxes_{{uniqueid}}"></label>
            </div>

            <div class="heading-title type-cell text-center px-3 flex-grow-0 border-right border-secondary"
                role="heading">
                {{#str}}qtype, community_sharequestion{{/str}}
            </div>

            <div class="heading-title px-2 name-cell flex-grow-1 border-right border-secondary" role="heading">
                {{#str}}qcontent, community_sharequestion{{/str}}
            </div>

            <div class="heading-title px-2 text-center create-cell flex-grow-0 border-right border-secondary"
                role="heading">
                {{#str}}qcreatedby, community_sharequestion{{/str}}
            </div>

            <div class="heading-title px-2 text-center create-date-cell flex-grow-0 border-right border-secondary"
                role="heading">
                {{#str}}qcreateddate, community_sharequestion{{/str}}
            </div>

            <div class="heading-title px-2 text-center update-date-cell flex-grow-0 border-right border-secondary"
                role="heading">
                {{#str}}qupdateddate, community_sharequestion{{/str}}
            </div>

            <div class="heading-title px-2 text-center last-cell flex-grow-0" role="heading"></div>
        </div>

        <div id="popup_block_content_courses_{{uniqueid}}" class="popup_block_content"></div>

    </div>
</div>

{{#js}}
    require(['community_sharequestion/copyFromMyCourses', 'jquery'], function(copyFromMyCourses, $) {
        copyFromMyCourses.actionsMain();
    });
{{/js}}